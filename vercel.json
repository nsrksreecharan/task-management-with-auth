{
    "version":2,
    "builds":[
        {
            "src":"./server.js",
            "use":"@vercel/node"
        }
    ],
    "env": {
        "MONGODB_URI": "MONGODB_URI",
        "JWT_SECRET":"JWT_SECRET"
    },
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/server.js"
        }
    ]
}
